void handle_gun_firing() {
  int reading = digitalRead(GUN_TRIGGER_PIN);

  if (reading != last_gun_trigger_pin_state) {
    // Change detected, reset debounce timer
    last_debounce_time_gun_trigger = millis();
  }

  if ((millis() - last_debounce_time_gun_trigger) > debounce_delay_gun_trigger) {
      // Whatever reading is, it's been there longer than debounce delay. Take the reading as is
      if (reading != current_gun_trigger_pin_state) {
        // assert that reading is NOT due to debouncing
        current_gun_trigger_pin_state = reading;
//        ammo_remaining = (ammo_remaining == 0 && current_gun_trigger_pin_state == HIGH) ? 0 :
//                         (current_gun_trigger_pin_state == LOW) ? ammo_remaining : ammo_remaining - 1;

        if (current_gun_trigger_pin_state == HIGH) {
          IR_transmit();
          shot_fired = 1;
          sendBeetleData(5); // BEETLE NUMBER
          lastPacketSentTime = millis();
          shot_fired = 0;
        }
    }

    // Display bullet count
//    display.clearDisplay();
//    display.drawBitmap(0, 0, image_bullets[ammo_remaining], 128, 64, 1);
//    display.display();
  }
  last_gun_trigger_pin_state = reading;
}

void handle_reset_switch() {
  int reading = digitalRead(RESET_SWITCH);

  if (reading != last_reset_switch_state) {
    // Change detected, reset debounce timer
    last_debounce_time_reset_switch = millis();
  }

  if ((millis() - last_debounce_time_reset_switch) > debounce_delay_reset_switch) {
      // Whatever reading is, it's been there longer than debounce delay. Take the reading as is
      if (reading != current_reset_switch_state) {
        // assert that reading is NOT due to debouncing
        current_reset_switch_state = reading;
        ammo_remaining = MAX_NUM_AMMO;
      }
  }

  // Display bullet count
  display.clearDisplay();
  display.drawBitmap(0, 0, image_bullets[ammo_remaining], 128, 64, 1);
  display.display();

  last_reset_switch_state = reading;
}


void IR_transmit() {
  // Peak forward current = 1A
  // Continuous Forward Current = 100mA
  #ifdef FLASH_PLAYER_ONE_RED_BASE
    IrSender.sendNEC(0x100, 0x35, 0); // the address 0x0100 with the command 0x200 is sent ONCE for Player 1
  #endif

  #ifdef FLASH_PLAYER_TWO_BLACK_BASE
    IrSender.sendNEC(0x101, 0x36, 0); // the address 0x0100 with the command 0x200 is sent ONCE for Player 2
  #endif
}

void OLED_init() {
  // Begin I2C comms; Beetle(Master) -> OLED(Slave)
  //Serial.begin(9600);

  // Show Adafruit logo on OLED (sanity check)
  display.begin(SH1106_SWITCHCAPVCC, 0x3C); // Generate HIGH voltage from 3.3V internally, and initialize with I2C address of 0x3C for 128x64 display we are using
  display.display();                        // Image buffer is Adafruit splashscreen
  delay(1000);
  display.clearDisplay();                   // Clear buffer
}

void gun_init() {
  // GUN_TRIGGER_PIN is active high, (i.e. GUN_TRIGGER_PIN reads HIGH when gun trigger is squeezed)
  // GUN_TRIGGER_PIN is connected to GND initially, INPUT_PULLUP is to prevent floating reads
  // When gun trigger is pressed, GUN_TRIGGER_PIN connects to Vdd
  pinMode(GUN_TRIGGER_PIN, INPUT_PULLUP);

  // Global counter to keep track of how much ammo left, wraps around from 0->6
  ammo_remaining = MAX_NUM_AMMO;

  // Debouncing the gun trigger
  current_gun_trigger_pin_state = HIGH; // Pin is in active high configuration
  last_gun_trigger_pin_state = HIGH;    // Pin is in active high configuration
  last_debounce_time_gun_trigger = 0;
  debounce_delay_gun_trigger = 100;
}

void IR_transmit_init() {
  IrSender.begin(IR_TRANSMIT_PIN);
}

void reset_switch_init() {
  pinMode(RESET_SWITCH, INPUT_PULLUP);

  // Debouncing the reset trigger
  current_reset_switch_state = HIGH; // Pin is in active high configuration
  last_reset_switch_state = HIGH;    // Pin is in active high configuration
  last_debounce_time_reset_switch = 0;
  debounce_delay_reset_switch = 100;
}

//========================Calculate CRC======================

uint32_t calculateDataCrc32(Datapacket *pkt) {
  return custom_crc32(&pkt->type, 
    sizeof(pkt->type) +
    sizeof(pkt->bullet_count) +
    sizeof(pkt->shot_fired) +
    sizeof(pkt->padding_1) +
    sizeof(pkt->padding_2) +
    sizeof(pkt->padding_3) +
    sizeof(pkt->padding_4) +
    sizeof(pkt->sequence_number) +
    sizeof(pkt->padding_5)
  );
}

uint32_t calculateAckCrc32(Ackpacket *pkt) {
  return custom_crc32(&pkt->type, 
    sizeof(pkt->type) +
    sizeof(pkt->padding_1) +
    sizeof(pkt->padding_2) +
    sizeof(pkt->padding_3) +
    sizeof(pkt->padding_4) +
    sizeof(pkt->padding_5) +
    sizeof(pkt->padding_6) +
    sizeof(pkt->padding_7) +
    sizeof(pkt->padding_8)
  );
}

//===========================================================

//====================Custom CRC Library======================


uint32_t custom_crc32(const uint8_t *data, size_t len) {
  uint32_t crc = 0x00000000;
  
  for (size_t i = 0; i < len; i++) {
    crc ^= data[i];
    for (int j = 0; j < 8; j++) {
      crc = (crc & 1) ? ((crc >> 1) ^ 0x04C11DB7) : (crc >> 1);
    }
  }
  
  return crc;
}
