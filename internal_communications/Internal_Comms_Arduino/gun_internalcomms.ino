void setup()
{
  Serial.begin(115200);
  OLED_init();
  gun_init();
  reset_switch_init();
  resetFlags();
}

void loop()
{
  if (Serial.available()) {
    if (Serial.peek() == 'g') 
      currentState = Serial.read();
    if (Serial.peek() == 'h') 
      currentState = Serial.read();
  }

  if (currentState != 'x') {
    switch(currentState) {
      case 's':
        handle_gun_firing();
        handle_reset_switch();
//        sendBeetleData(5); // BEETLE NUMBER
//        lastPacketSentTime = millis();
//        shot_fired = 0; // reset shot_fired everytime packet sends, regardless.
        display.clearDisplay();
        display.drawBitmap(0, 0, image_bullets[ammo_remaining], 128, 64, 1);
        display.display();
        break;
      case 'g':
        if (Serial.available() >= sizeof(uint8_t)) {
          uint8_t receivedByte = Serial.read();
          ammo_remaining = receivedByte;
          setStateToSend();
        }
        break;
      case 'h':
        resetFlags();
        sendHandshakeAck();
        waitForHandshakeAck();
        break;
      case 'i':
        IR_transmit_init();
        delay(1000);
        setStateToSend();
        break;
      default:
        resetFlags();
        break;
    }
    
  }
}

